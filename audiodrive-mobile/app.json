{
    "expo": {
        "name": "audiodrive-mobile",
        "slug": "audiodrive-mobile",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./mobile/assets/icon.png",
        "splash": {
            "image": "./mobile/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./mobile/assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./mobile/assets/favicon.png"
        },
        "entryPoint": "./mobile/App.tsx"
    }
}